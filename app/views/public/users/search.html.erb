<section class="inner">
  <h2 class="section-title">ユーザー検索</h2>
  <%= render 'search', q: @q, url: users_path %>
  <table class="users has_border">
    <thead>
      <tr>
        <th></th>
        <th>ユーザーネーム</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_image(100,100) %></td>
        <td><%= user.name %></td>
        <td></td>
      </tr>
    </tbody>
  </table>
</section>